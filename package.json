{
  "name": "clean-node-api",
  "version": "2.5.7",
  "description": "Node.js Rest API using TDD, Clean Architecture and Typesript",
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "node src/main/server.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "local": "npm run build && npm run start",
    "up": "npm run build && docker compose up",
    "down": "sudo docker compose down",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < ./coverage/lcov.info"
  },
  "keywords": [],
  "author": "JoÃ£o Feitoza",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@shelf/jest-mongodb": "^4.1.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "bson-objectid": "^2.0.4",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.30.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^4.4.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.13.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "18.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
